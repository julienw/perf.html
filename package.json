{
  "name": "perf.html",
  "version": "0.0.1",
  "description": "perf.html, the gecko profiler UI",
  "main": "src/index.js",
  "scripts": {
    "build": "yarn run build:quiet -- --progress",
    "build:quiet": "yarn run build:clean && NODE_ENV=development webpack",
    "build:clean": "rimraf dist && mkdirp dist && cp res/.htaccess dist/ && cp res/zee-worker.js dist/",
    "build-prod": "yarn run build:clean && NODE_ENV=production webpack -p --progress",
    "build-prod-readable": "yarn run build:clean && NODE_ENV=production webpack --progress",
    "build-docs": "documentation build src/types/profile.js -f md > docs/processed-profile-format.md; cat docs/processed-profile-format.md",
    "lint": "eslint *.js bin src",
    "lint-fix": "yarn lint -- --fix",
    "flow": "flow",
    "flow-coverage": "flow-coverage-report -i 'src/**/*.js' -t html -t text",
    "flow-generate-libdefs": "flow-typed install",
    "license-check": "devtools-license-check",
    "postcheckout": "node bin/post-checkout.js",
    "postmerge": "node bin/post-merge.js",
    "precommit": "lint-staged",
    "prepush": "yarn run test-all",
    "postrewrite": "node bin/post-rewrite.js",
    "publish": "rimraf public_html && cp -r dist public_html",
    "serve-static": "ws -d dist/ -s index.html -p 4242",
    "start": "mkdir -p dist && cp res/zee-worker.js dist/ && NODE_ENV=development node server.js",
    "start-no-hot": "yarn run start --",
    "start-prod": "yarn run build-prod && yarn run serve-static --",
    "start-prod-readable": "yarn run build-prod-readable && yarn run serve-static --",
    "start-examples": "ws -d examples/ -s index.html -p 4242",
    "test": "NODE_ENV=test jest",
    "test-all": "run-p flow license-check lint test"
  },
  "author": "Markus Stange <mstange@themasta.com>",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/devtools-html/perf.html"
  },
  "dependencies": {
    "array-move": "^1.0.0",
    "array-range": "^1.0.1",
    "babel-runtime": "^6.23.0",
    "bisection": "0.0.3",
    "clamp": "^1.0.1",
    "classnames": "^2.2.5",
    "common-tags": "^1.4.0",
    "copy-to-clipboard": "^3.0.5",
    "fetch-jsonp": "^1.0.2",
    "offline-plugin": "^4.5.2",
    "pretty-bytes": "^4.0.2",
    "prop-types": "^15.5.10",
    "query-string": "^4.2.3",
    "react": "^15.6.1",
    "react-addons-perf": "^15.4.2",
    "react-contextmenu": "^2.6.3",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-transition-group": "^1.2.0",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "text-encoding": "^0.6.4",
    "url": "^0.11.0"
  },
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current",
            "browsers": "defaults"
          }
        }
      ],
      "flow"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.28.4",
    "devtools-license-check": "^0.2.0",
    "eslint": "^4.1.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^7.1.0",
    "express": "^4.15.3",
    "fake-indexeddb": "^2.0.3",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.48.0",
    "flow-coverage-report": "^0.3.0",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.24.1",
    "http-server": "^0.9.0",
    "husky": "^0.14.1",
    "jest": "^20.0.3",
    "json-loader": "^0.5.4",
    "lint-staged": "^4.0.0",
    "local-web-server": "^1.2.6",
    "lodash.clonedeep": "^4.5.0",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "prettier": "^1.5.1",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.5.4",
    "sinon": "^2.1.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "workerjs": "github:jasonLaster/workerjs"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/fixtures/mocks/file-mock.js",
      "\\.(css|less)$": "<rootDir>/src/test/fixtures/mocks/style-mock.js"
    },
    "setupTestFrameworkScriptFile": "./src/test/setup.js",
    "verbose": true
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
