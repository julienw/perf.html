{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { loose: true }],
    [ "@babel/plugin-proposal-object-rest-spread", { "useBuiltIns": true } ]
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-flow"
      ],
      "plugins": [
        ["@babel/plugin-proposal-class-properties", { loose: true }],
        [ "@babel/plugin-proposal-object-rest-spread", { "useBuiltIns": true } ]
      ]
    }
  }
}
